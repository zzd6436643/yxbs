<%@ page contentType="text/html;charset=UTF-8"%><%@ include file="/WEB-INF/jsp/common/taglibs.jsp"%><script src="${ctx}/resources/js/lib/bootstrap-paginator/bootstrap-paginator.js" type="text/javascript"></script><link href="${ctx}/resources/js/lib/bootstrap-datepicker/bootstrap-datepicker.css" rel="stylesheet"><script src="${ctx}/resources/js/lib/bootstrap-datepicker/locale/bootstrap-datepicker.zh-CN.js" type="text/javascript" charset="UTF-8"></script><script src="${ctx}/resources/js/page/userMgr.js" type="text/javascript"></script><div class="row-fluid">	<div class="span11" style="padding-left: 15px;">		<div style="padding-top: 15px;">			<form id="userListForm" action='${ctx}/userMgr/main' method='POST'>				<table>					<tr>						<td><label for="userId">用户编码</label></td>						<td style="padding-left: 2px;"><input id="userId" name="userId" type="text" value='<c:out value="${userCondition.userId}"  escapeXml="true"></c:out>'></td>						<td style="padding-left: 10px;"><label for="username">用户名</label></td>						<td style="padding-left: 2px;"><input id="username" name="username" type="text" value='<c:out value="${userCondition.username}"  escapeXml="true"></c:out>'></td>						<td style="padding-left: 10px;"><label for="displayIsenable">用户状态</label></td>						<td style="padding-left: 2px;"><input id="displayIsenable" name="displayIsenable" type="text" value="${userCondition.displayIsenable}"></td>					</tr>					<tr>						<td><label for="role">用户级别</label></td>						<td style="padding-left: 2px;"><input id="role" name="role" type="text" value='<c:out value="${userCondition.role}"  escapeXml="true"></c:out>'></td>						<td colspan="4" style="padding-left:10px;">							<div style="float:left;">								<span>创建时间</span>							</div>							<div style="float:left;padding-left:2px;width:210px;">								<input type="text" id="dps" name="displayBeginDate" style="width:85px;" value="${userCondition.displayBeginDate}">  								<input type="text" id="dpe" name="displayEndDate" style="width:85px;" value="${userCondition.displayEndDate}"> 							</div>							<div id="dpAlert" class="alert alert-error" style="float:left;padding:3px 20px 3px 14px;margin-bottom:10px;">								<strong>Hello world!</strong>							</div>						</td>					</tr>				</table>				<input id="pageIndex" name="pageIndex" type="hidden"> <input id="pageSize" name="pageSize" type="hidden" value="${userCondition.pageSize}">				<div style="padding-left: 200px;">					<button class="btn btn-primary" type="submit" id="btnQuery">查询</button>					<button class="btn btn-primary" style="margin-left: 50px;" id="btnReset">重置</button>				</div>			</form>			<input id="totalCount" type="hidden" value="${totalCount}"> 			<input id="currentPage" type="hidden" value="${currentPage}"> 			<input id="totalPage" type="hidden" value="${totalPage}">		</div>		<div style="padding-top: 10px;">用户列表:</div>		<div style="padding-top: 10px;">			<display:table name="resultList" id="row" class="table table-striped table-bordered" style="text-align:center;" cellspacing="0" cellpadding="0">				<display:column property="userId" title="用户编码" />				<display:column property="username" title="用户帐号" escapeXml="true" />				<display:column property="role" title="角色级别" />				<c:choose>					<c:when test="${row.displayIsenable eq 'enabled'}">						<display:column property="displayIsenable" title="用户状态" style="color:green;" />					</c:when>					<c:otherwise>						<display:column property="displayIsenable" title="用户状态" style="color:red;" />					</c:otherwise>				</c:choose>				<display:column property="createDate" title="创建时间" format="{0,date,yyyy-MM-dd HH:mm:ss}" />				<display:column property="modifyDate" title="最近修改时间" format="{0,date,yyyy-MM-dd HH:mm:ss}" />				<display:column title="操作"></display:column>			</display:table>			<div id="pagination" style="float:left;"></div>			<div id="paginationCount" style="float:left; margin-top: 30px; margin-left: 10px;">				total: <span style="color: blue;">${totalCount} records</span>			</div>		</div>	</div></div>