<%@ page contentType="text/html;charset=UTF-8"%><%@ include file="/WEB-INF/jsp/common/taglibs.jsp"%><script src="${ctx}/resources/js/lib/bootstrap-paginator/bootstrap-paginator.js" type="text/javascript"></script><link href="${ctx}/resources/js/lib/bootstrap-datepicker/bootstrap-datepicker.css" rel="stylesheet"><script src="${ctx}/resources/js/lib/bootstrap-datepicker/locale/bootstrap-datepicker.zh-CN.js" type="text/javascript" charset="UTF-8"></script><script src="${ctx}/resources/js/page/userMgr.js" type="text/javascript"></script><div class="row-fluid">	<div class="span11" style="padding-left: 15px;">		<div style="padding-top: 15px;">			<form id="userListForm" action='${ctx}/userMgr/main' method='POST'>				<table>					<tr>						<td><label for="userId">用户编码</label></td>						<td style="padding-left: 2px;"><input id="userId" name="userId" type="text" autocomplete="off" value='<c:out value="${userCondition.userId}"   escapeXml="true"></c:out>'></td>						<td style="padding-left: 10px;"><label for="username">用户名</label></td>						<td style="padding-left: 2px;"><input id="username" name="username" type="text" autocomplete="off"							value='<c:out value="${userCondition.username}"  escapeXml="true"></c:out>'></td>						<td style="padding-left: 10px;"><label for="displayIsenable">用户状态</label></td>						<td style="padding-left: 2px;"><input id="displayIsenable" name="displayIsenable" type="text" data-provide="typeahead" autocomplete="off"							value="${userCondition.displayIsenable}"></td>					</tr>					<tr>						<td><label for="role">用户级别</label></td>						<td style="padding-left: 2px;"><input id="role" name="role" type="text" data-provide="typeahead" autocomplete="off"							value='<c:out value="${userCondition.role}" escapeXml="true"></c:out>'></td>						<td colspan="4" style="padding-left: 10px;">							<div style="float: left;">								<span>创建时间</span>							</div>							<div style="float: left; padding-left: 2px; width: 210px;">								<input type="text" id="dps" name="displayBeginDate" style="width: 85px;" autocomplete="off" value="${userCondition.displayBeginDate}"> <input type="text" id="dpe"									name="displayEndDate" style="width: 85px;" autocomplete="off" value="${userCondition.displayEndDate}">							</div>							<div id="dpAlert" class="alert alert-error" style="float: left; padding: 3px 20px 3px 14px; margin-bottom: 10px;">								<strong>Hello world!</strong>							</div>						</td>					</tr>				</table>				<input id="pageIndex" name="pageIndex" type="hidden"> <input id="pageSize" name="pageSize" type="hidden" value="${userCondition.pageSize}">				<div style="padding-left: 210px;">					<button class="btn btn-primary" type="submit" id="btnQuery">查询</button>					<button class="btn btn-primary" type="button" style="margin-left: 50px;" id="btnReset">重置</button>				</div>			</form>			<input id="totalCount" type="hidden" value="${totalCount}"> <input id="currentPage" type="hidden" value="${currentPage}"> <input id="totalPage" type="hidden"				value="${totalPage}"> <input id="ctx" type="hidden" value="${ctx}">		</div>		<div style="margin-top: 5px;">			<a href="#newUserDialog" role="button" class="btn btn-primary btn-small" data-toggle="modal">新增</a>			<a href="#deprecatedUserDialog" role="button" class="btn btn-primary btn-small" data-toggle="modal">废弃</a>			<a href="#activateUserDialog" role="button" class="btn btn-primary btn-small" data-toggle="modal">激活</a>		</div>		<div style="padding-top: 10px;">用户列表:</div>		<div style="padding-top: 10px;">			<display:table name="resultList" id="row" class="table table-striped table-bordered">				<display:column property="userId" title="用户编码" />				<display:column property="username" title="用户帐号" escapeXml="true" />				<display:column property="role" title="角色级别" />				<c:choose>					<c:when test="${row.displayIsenable eq 'enabled'}">						<display:column property="displayIsenable" title="用户状态" style="color:green;" />					</c:when>					<c:otherwise>						<display:column property="displayIsenable" title="用户状态" style="color:red;" />					</c:otherwise>				</c:choose>				<display:column property="createDate" title="创建时间" format="{0,date,yyyy-MM-dd HH:mm:ss}" />				<display:column property="modifyDate" title="最近修改时间" format="{0,date,yyyy-MM-dd HH:mm:ss}" />				<display:column title="操作">					<a href="#modifyPasswdDialog" role="button" class="btn btn-primary btn-small modifyA" tag= ${row.userId} data-toggle="modal" >修改密码</a>					<button style="margin-left: 10px;" class="btn btn-danger btn-small delUserBtn" type="button" tag= ${row.userId} >删除</button>				</display:column>			</display:table>			<div id="pagination" style="float: left;"></div>			<div id="paginationCount" style="float: left; margin-top: 30px; margin-left: 10px;">				total: <span style="color: blue;">${totalCount} records</span>			</div>		</div>	</div></div><div id="newUserDialog" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="newUserLabel" aria-hidden="true">	<div class="modal-header">		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>		<h3 id="newUserLabel">新增用户</h3>	</div>	<div class="modal-body">		<form id="newUserForm" name="newUserForm" method="post">			<table>				<tr>					<td>用户名</td>					<td><input type='text' id='new_username' name='new_username'></td>				</tr>				<tr>					<td>密码</td>					<td><input type='text' id='new_passwd' name='new_passwd' /></td>				</tr>			</table>		</form>	</div>	<div class="modal-footer">		<button data-dismiss="modal" aria-hidden="true" class="btn btn-primary">关闭</button>		<button id="newUserBtn" class="btn btn-success">新增</button>	</div></div><div id="modifyPasswdDialog" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="modifyPasswdLabel" aria-hidden="true">	<div class="modal-header">		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>		<h3 id="modifyPasswdLabel">修改密码</h3>	</div>	<div class="modal-body">		<form id="modifyPasswdForm" name="modifyPasswdForm" method="post">			<table>				<tr>					<td>新密码</td>					<td><input type='text' id='modify_passwd' name='modify_passwd'></td>					<input type="hidden" id='modify_userId' name='modify_userId'>				</tr>			</table>		</form>	</div>	<div class="modal-footer">		<button data-dismiss="modal" aria-hidden="true" class="btn btn-primary">关闭</button>		<button id="modifyPasswdBtn" class="btn btn-info">修改</button>	</div></div><div id="deprecatedUserDialog" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="deprecatedUserLabel" aria-hidden="true">	<div class="modal-header">		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>		<h3 id="deprecatedUserLabel">废弃用户</h3>	</div>	<div class="modal-body">		<form id="deprecatedUserForm" name="deprecatedUserForm" method="post">			<table>				<tr>					<td>用户名</td>					<td><input type='text' id='deprecated_user' name='deprecated_user'></td>				</tr>			</table>		</form>	</div>	<div class="modal-footer">		<button data-dismiss="modal" aria-hidden="true" class="btn btn-primary">关闭</button>		<button id="deprecatedUserBtn" class="btn btn-warning">废弃</button>	</div></div><div id="activateUserDialog" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="activateUserLabel" aria-hidden="true">	<div class="modal-header">		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>		<h3 id="activateUserLabel">激活用户</h3>	</div>	<div class="modal-body">		<form id="activateUserForm" name="activateUserForm" method="post">			<table>				<tr>					<td>用户名</td>					<td><input type='text' id='activate_user' name='activate_user'></td>				</tr>			</table>		</form>	</div>	<div class="modal-footer">		<button data-dismiss="modal" aria-hidden="true" class="btn btn-primary">关闭</button>		<button id="activateUserBtn" class="btn btn-info">激活</button>	</div></div>