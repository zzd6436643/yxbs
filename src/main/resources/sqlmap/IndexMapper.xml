<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="org.jzw.yxbs.dao.IndexDao">	<select id="getUserAttributeVal" resultType="java.lang.String">		select		attributeval		from		sys_user_info		<where>			username = #{username} and attributeid = #{userInfoid}		</where>	</select>	<update id="updateUserAttributeVal" parameterType="org.jzw.yxbs.condition.UserInfoCondition">		update		sys_user_info		set attributeval = #{userInfoval}		<where>			username = #{username} and attributeid = #{userInfoid}		</where>	</update>	<insert id="insertNewUserInfo" parameterType="org.jzw.yxbs.condition.UserInfoCondition">		insert into		sys_user_info(username,attributeid,attributeval)		values		(#{username},#{userInfoid},#{userInfoval})	</insert>	<insert id="insertNewUserThisLoginTime" parameterType="org.jzw.yxbs.condition.UserInfoCondition">		insert into		sys_user_info(username,attributeid,attributeval)		values		(#{username},#{userInfoid},date_format(now(),'%Y-%c-%d %h:%i:%s'))	</insert></mapper>